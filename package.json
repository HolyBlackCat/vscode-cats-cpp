{
    "name": "cats-cpp",
    "displayName": "Clean C++ Syntax",
    "description": "An alternative no-nonsense C++ syntax",
    "version": "0.0.37",
    "publisher": "holyblackcat",
    "icon": "logo.png",
    "repository": {
		"type": "git",
		"url": "https://github.com/HolyBlackCat/vscode-cats-cpp"
	},
    "engines": {
        "vscode": "^1.32.0"
    },
    "categories": [
        "Programming Languages", "Themes"
    ],
    "contributes": {
        "languages": [{
            "id": "cpp",
            "aliases": ["C++'", "cpp"],
            "extensions": [".cpp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "cpp",
            "scopeName": "source.catscpp",
            "path": "./syntaxes/cats-cpp.tmLanguage.json"
        }],
		"themes": [
            {
                "label": "Clean C++ Light",
                "uiTheme": "vs",
                "path": "./themes/cats-light.json"
            }
        ]
    }
}
