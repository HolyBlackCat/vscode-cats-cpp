{
	"$schema": "vscode://schemas/color-theme",
	"name": "Cat's Light",
	"type": "light",
	"colors": {
		"editor.background": "#ffffff",
        "editor.foreground": "#000000",
    },
	"tokenColors": [
        {
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#ff3769"
			}
		},
		{
			"name": "Comments",
			"scope": [
                "comment",
                "comment.block",
                "punctuation.definition.comment"
            ],
			"settings": {
				"fontStyle": "",
				"foreground": "#929292"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.line.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#be856e"
			}
        },
        {
			"name": "Preprocessor",
            "scope": [
                "meta.pp",
                "keyword.operator.pp",
                "keyword.other.pp",
                "comment.block.preprocessor"
            ],
			"settings": {
				"foreground": "#dd7b0a"
			}
        },
        {
			"name": "Preprocessor (system header)",
            "scope": [
                "string.quoted.double.include.system",
                "string.quoted.double.quote.include.system"
            ],
			"settings": {
				"foreground": "#b14100"
			}
		},
        {
			"name": "Preprocessor (user header)",
            "scope": [
                "string.quoted.double.include.user",
                "string.quoted.double.quote.include.user"
            ],
			"settings": {
				"foreground": "#b14100"
			}
        },
        {
			"name": "Preprocessor (pragma)",
            "scope": [
                "meta.pp.pragma_value"
            ],
			"settings": {
				"foreground": "#813103"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#000000"
			}
        },
        {
			"name": "Operators (words)",
			"scope": "keyword.operator.word",
			"settings": {
				"foreground": "#ff00c8"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#0050c7"
			}
        },
        {
			"name": "Keyword: throw",
			"scope": [
				"keyword.control.oneline.throw"
			],
			"settings": {
				"foreground": "#ff0000"
			}
        },
        {
			"name": "Keywords: types",
			"scope": [
				"storage.type.type"
			],
			"settings": {
				"foreground": "#0a8a8f"
			}
		},
		{
			"name": "Keywords: storage categories",
			"scope": [
                "support.type",
                "storage.type",
				"storage.type.category"
			],
			"settings": {
				"foreground": "#97281c"
			}
        },
        {
			"name": "Keywords: context-sensitive",
			"scope": [
				"keyword.other.context"
			],
			"settings": {
				"foreground": "#db0054"
			}
        },
        {
			"name": "Keywords: cv-qualifiers",
			"scope": [
				"storage.modifier.cv"
			],
			"settings": {
				"foreground": "#0050c7"
			}
        },
        {
			"name": "Attributes",
			"scope": [
				"meta.attribute.region"
			],
			"settings": {
				"foreground": "#640046"
			}
		},
        {
			"name": "Attributes (names)",
			"scope": [
				"storage.modifier.attrib"
			],
			"settings": {
				"foreground": "#ff25be"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#200dc4"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#058516"
			}
        },
        {
			"name": "Characters",
			"scope": [
				"constant.character",
			],
			"settings": {
				"foreground": "#ee4816"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#ca0000"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#520000"
			}
		},
	]
}