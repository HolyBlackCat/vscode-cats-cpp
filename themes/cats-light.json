{
	"$schema": "vscode://schemas/color-theme",
	"name": "Cat's Light",
	"type": "light",
	"colors": {
		"editor.background": "#ffffff",
        "editor.foreground": "#000000",
    },
	"tokenColors": [
        {
			"name": "Invalid & illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#ff3769"
			}
		},
		{
			"name": "Comments",
			"scope": [
                "comment",
                "comment.block",
                "punctuation.definition.comment"
            ],
			"settings": {
				"fontStyle": "",
				"foreground": "#929292"
			}
		},
		{
			"name": "Comments: documentation",
			"scope": [
				"comment.documentation",
				"comment.line.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#be856e"
			}
        },
        {
			"name": "Preprocessor",
            "scope": [
                "meta.pp",
                "keyword.operator.pp",
                "keyword.other.pp",
                "comment.block.preprocessor"
            ],
			"settings": {
				"foreground": "#dd7b0a"
			}
        },
        {
			"name": "Preprocessor (system header)",
            "scope": [
                "string.quoted.double.include.system",
                "string.quoted.double.quote.include.system"
            ],
			"settings": {
				"foreground": "#b14100"
			}
		},
        {
			"name": "Preprocessor (user header)",
            "scope": [
                "string.quoted.double.include.user",
                "string.quoted.double.quote.include.user"
            ],
			"settings": {
				"foreground": "#b14100"
			}
        },
        {
			"name": "Preprocessor (pragma)",
            "scope": [
                "meta.pp.pragma_value"
            ],
			"settings": {
				"foreground": "#813103"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#000000"
			}
        },
        {
			"name": "Operators (words)",
			"scope": "keyword.operator.word",
			"settings": {
				"foreground": "#ff00c8"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#0050c7"
			}
        },
        {
			"name": "Keywords: types",
			"scope": [
				"storage.type.type"
			],
			"settings": {
				"foreground": "#0a8a8f"
			}
		},
		{
			"name": "Keywords: storage categories",
			"scope": [
                "support.type",
                "storage.type",
				"storage.type.category"
			],
			"settings": {
				"foreground": "#97281c"
			}
        },
        {
			"name": "Keywords: context-sensitive",
			"scope": [
				"keyword.other.context"
			],
			"settings": {
				"foreground": "#db0054"
			}
        },
        {
			"name": "Keywords: cv-qualifiers",
			"scope": [
				"storage.modifier.cv"
			],
			"settings": {
				"foreground": "#0050c7"
			}
        },
        {
			"name": "Attributes",
			"scope": [
				"meta.attribute.region"
			],
			"settings": {
				"foreground": "#640046"
			}
		},
        {
			"name": "Attributes (names)",
			"scope": [
				"storage.modifier.attrib"
			],
			"settings": {
				"foreground": "#ff25be"
			}
		},
		{
			"name": "Language constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#200dc4"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#058516"
			}
        },
        {
			"name": "Characters",
			"scope": [
				"constant.character",
			],
			"settings": {
				"foreground": "#ee4816"
			}
		},
		{
			"name": "Strings",
            "scope": [
                "string",
                "string.quoted"
            ],
			"settings": {
				"foreground": "#c51f1f"
			}
        },
        {
			"name": "Strings: raw",
			"scope": "string.quoted.triple",
			"settings": {
				"foreground": "#49493c"
			}
        },
        {
			"name": "Strings: raw (delimiter)",
			"scope": "string.quoted.triple.delim",
			"settings": {
				"foreground": "#d16b17"
			}
		},
		{
			"name": "Strings: escape sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#520000"
			}
        },
        {
			"name": "Qualifiers: scopes",
			"scope": "meta.qualifier.scope",
			"settings": {
				"foreground": "#80521d"
			}
        },
        {
			"name": "Qualifiers: objects",
			"scope": "meta.qualifier.object",
			"settings": {
				"foreground": "#5c5c5c"
			}
        },
        {
			"name": "Qualifiers: pointers",
			"scope": "meta.qualifier.pointer",
			"settings": {
				"foreground": "#5c5c5c"
			}
        },
        {
			"name": "Qualifiers: additional information",
			"scope": "meta.qualifier.addition",
			"settings": {
				"foreground": "#b80156"
			}
        }
	]
}