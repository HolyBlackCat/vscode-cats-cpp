{
	"$schema": "vscode://schemas/color-theme",
	"name": "Cat's Light",
	"type": "light",
	"colors": {
        "editor.background": "#FFFFFF",
		"editor.foreground": "#000000",
		"editor.inactiveSelectionBackground": "#E5EBF1",
		"editorIndentGuide.background": "#D3D3D3",
		"editorIndentGuide.activeBackground": "#939393",
		"editor.selectionHighlightBackground": "#ADD6FF80",
		"editorSuggestWidget.background": "#F3F3F3",
		"activityBarBadge.background": "#007ACC",
		"sideBarTitle.foreground": "#6F6F6F",
		"list.hoverBackground": "#E8E8E8",
		"input.placeholderForeground": "#767676",
		"settings.textInputBorder": "#CECECE",
		"settings.numberInputBorder": "#CECECE"
    },
	"tokenColors": [
        {
			"name": "Invalid & illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#ff3769"
			}
		},
		{
			"name": "Comments",
			"scope": [
                "comment",
                "comment.block",
                "punctuation.definition.comment"
            ],
			"settings": {
				"fontStyle": "",
				"foreground": "#929292"
			}
		},
		{
			"name": "Comments: documentation",
			"scope": [
				"comment.documentation",
				"comment.line.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#be856e"
			}
        },
        {
			"name": "Preprocessor",
            "scope": [
                "meta.pp",
                "keyword.operator.pp",
                "keyword.other.pp",
                "comment.block.preprocessor"
            ],
			"settings": {
				"foreground": "#dd7b0a"
			}
        },
        {
			"name": "Preprocessor (system header)",
            "scope": [
                "string.quoted.double.include.system",
                "string.quoted.double.quote.include.system"
            ],
			"settings": {
				"foreground": "#0d7527"
			}
		},
        {
			"name": "Preprocessor (user header)",
            "scope": [
                "string.quoted.double.include.user",
                "string.quoted.double.quote.include.user"
            ],
			"settings": {
				"foreground": "#2a37e6"
			}
        },
        {
			"name": "Preprocessor (pragma)",
            "scope": [
                "meta.pp.pragma_value"
            ],
			"settings": {
				"foreground": "#db240c"
			}
        },
        {
			"name": "Preprocessor (constants)",
			"scope": [
				"constant.language.macro"
			],
			"settings": {
				"foreground": "#8f00c7"
				// "foreground": "#200dc4"
			}
        },
        {
			"name": "Preprocessor (macro definition body)",
			"scope": [
				"meta.pp.line.define"
			],
			"settings": {
				"foreground": "#472a05"
			}
        },
        {
			"name": "Preprocessor (macro name)",
			"scope": [
				"meta.pp.macro_name"
			],
			"settings": {
				"foreground": "#b14100"
			}
        },
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#000000"
			}
        },
        {
			"name": "Operators (words)",
			"scope": "keyword.operator.word",
			"settings": {
				"foreground": "#ff00c8"
			}
		},
		{
			"name": "Keywords",
			"scope": [
                "support.type",
                "storage.type",
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#0050c7"
			}
        },
        // {
		// 	"name": "Keywords: types",
		// 	"scope": [
		// 		"storage.type.type"
		// 	],
		// 	"settings": {
		// 		"foreground": "#0a8a8f"
		// 	}
		// },
		{
			"name": "Keywords: storage categories",
			"scope": [
				"storage.type.category"
			],
			"settings": {
				"foreground": "#97281c"
			}
        },
        {
			"name": "Keywords: context-sensitive",
			"scope": [
				"keyword.other.context"
			],
			"settings": {
				"foreground": "#db0054"
			}
        },
        // {
		// 	"name": "Keywords: cv-qualifiers",
		// 	"scope": [
		// 		"storage.modifier.cv"
		// 	],
		// 	"settings": {
		// 		"foreground": "#0050c7"
		// 	}
        // },
        {
			"name": "Attributes",
			"scope": [
				"meta.attribute.region"
			],
			"settings": {
				"foreground": "#640046"
			}
		},
        {
			"name": "Attributes (names)",
			"scope": [
				"storage.modifier.attrib"
			],
			"settings": {
				"foreground": "#ff25be"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#0050c7"
				// "foreground": "#200dc4"
			}
        },
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#058516"
			}
        },
        {
			"name": "Characters",
			"scope": [
				"constant.character",
			],
			"settings": {
				"foreground": "#ee4816"
			}
		},
		{
			"name": "Strings",
            "scope": [
                "string",
                "string.quoted"
            ],
			"settings": {
				"foreground": "#c51f1f"
			}
        },
        {
			"name": "Strings: raw",
			"scope": "string.quoted.triple",
			"settings": {
				"foreground": "#49493c"
			}
        },
        {
			"name": "Strings: raw (delimiter)",
			"scope": "string.quoted.triple.delim",
			"settings": {
				"foreground": "#d16b17"
			}
		},
		{
			"name": "Strings: escape sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#520000"
			}
        },
        // {
		// 	"name": "Qualifiers: scopes",
		// 	"scope": "meta.qualifier.scope",
		// 	"settings": {
		// 		"foreground": "#80521d"
		// 	}
        // },
        // {
		// 	"name": "Qualifiers: objects",
		// 	"scope": "meta.qualifier.object",
		// 	"settings": {
		// 		"foreground": "#5c5c5c"
		// 	}
        // },
        // {
		// 	"name": "Qualifiers: pointers",
		// 	"scope": "meta.qualifier.pointer",
		// 	"settings": {
		// 		"foreground": "#5c5c5c"
		// 	}
        // },
        {
			"name": "Qualifiers: additional information",
			"scope": "meta.qualifier.addition",
			"settings": {
				"foreground": "#0050c7"
				// "foreground": "#b80156"
			}
        }
	]
}