{
    "$schema": "vscode://schemas/color-theme",
    "name": "Cat's Light",
    "type": "light",
    "colors": {
        "editor.background": "#ffffff",
        "editor.foreground": "#000000",
        "editor.inactiveSelectionBackground": "#e5ebf1",
        "editorIndentGuide.background1": "#d3d3d3",
        "editorIndentGuide.activeBackground1": "#939393",
        "editor.selectionHighlightBackground": "#add6ff80",
        "editorSuggestWidget.background": "#f3f3f3",
        "activityBarBadge.background": "#007acc",
        "sideBarTitle.foreground": "#6f6f6f",
        "list.hoverBackground": "#e8e8e8",
        "input.placeholderForeground": "#767676",
        "settings.textInputBorder": "#cecece",
        "settings.numberInputBorder": "#cecece",
        "editorError.foreground": "#ff0000",
        "editorOverviewRuler.errorForeground": "#ff0000bf",
        "editorWarning.foreground": "#00af00",
        "editorOverviewRuler.warningForeground": "#00af00bf",
        "editorInfo.foreground": "#3298d3",
        "editorOverviewRuler.infoForeground": "#3298d3bf",
        "editorOverviewRuler.findMatchForeground": "#ffaa00bf",
        "inputOption.activeBorder": "#99CAEB",
        "inputOption.activeBackground": "#ceeafd",
        "list.activeSelectionForeground": "#363636",
        "list.activeSelectionBackground": "#c0e1fc",
        "focusBorder": "#0060c050",
        "list.inactiveSelectionBackground": "#d8eaf7",
        "list.focusHighlightForeground": "#0060c0",
        "menu.separatorBackground": "#cccccc"
    },
    "tokenColors": [
        {
            "name": "Invalid & illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#ff3769"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "comment.block",
                "punctuation.definition.comment"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#419741"
            }
        },
        {
            "name": "Comments: documentation",
            "scope": [
                "comment.documentation",
                "comment.line.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#d84e87"
            }
        },
        {
            "name": "Preprocessor (directive)",
            "scope": [
                "meta.preprocessor",
                "keyword.operator.preprocessor",
                "keyword.other.preprocessor",
                "comment.block.preprocessor"
            ],
            "settings": {
                "foreground": "#df217a"
            }
        },
        {
            "name": "Preprocessor (system header)",
            "scope": [
                "string.quoted.double.include.system",
                "string.quoted.double.quote.include.system"
            ],
            "settings": {
                "foreground": "#4403be"
            }
        },
        {
            "name": "Preprocessor (user header)",
            "scope": [
                "string.quoted.double.include.user",
                "string.quoted.double.quote.include.user"
            ],
            "settings": {
                "foreground": "#276d0b"
            }
        },
        {
            "name": "Preprocessor (pragma body)",
            "scope": [
                "meta.preprocessor.pragma_value"
            ],
            "settings": {
                "foreground": "#0062bd"
            }
        },
        {
            "name": "Preprocessor (error string)",
            "scope": [
                "meta.preprocessor.error_value"
            ],
            "settings": {
                "foreground": "#464646" // Copied from a raw string.
            }
        },
        {
            "name": "Preprocessor (constants)",
            "scope": [
                "constant.language.macro"
            ],
            "settings": {
                "foreground": "#a55d00"
                // "foreground": "#8f00c7"
                // "foreground": "#200dc4"
            }
        },
        {
            "name": "Preprocessor (symbol operators)", // # and ##
            "scope": [
                "punctuation.preprocessor"
            ],
            "settings": {
                "foreground": "#a55d00"
            }
        },
        {
            "name": "Preprocessor (word operators)", // defined, __has_include, __has_cpp_attribute
            "scope": [
                "keyword.operator.word.preprocessor"
            ],
            "settings": {
                // "foreground": "#a55d00"
                "foreground": "#ce2216"
                // "foreground": "#0062bd"
                // "foreground": "#333f86"
                // "foreground": "#39458a"
                // "foreground": "#8f0043"
                // "foreground": "#df217a"
                // "foreground": "#b900b9"
            }
        },
        {
            "name": "Preprocessor (directive body)",
            "scope": [
                "meta.preprocessor.line"
            ],
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Preprocessor (macro name)",
            "scope": [
                "meta.preprocessor.macro_name"
            ],
            "settings": {
                "foreground": "#8f0043"
            }
        },
        {
            "name": "Preprocessor (unknown directive)",
            "scope": [
                "keyword.other.preprocessor.unknown",
                "keyword.operator.preprocessor.hash.unknown"
            ],
            "settings": {
                "foreground": "#9e24ce"
            }
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "name": "Operators (words)",
            "scope": "keyword.operator.word",
            "settings": {
                "foreground": "#ff00c8"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "support.type",
                "storage.type",
                "keyword",
                "storage"
            ],
            "settings": {
                "foreground": "#0050c7"
            }
        },
        {
            "name": "Keywords: storage categories",
            "scope": [
                "storage.type.category"
            ],
            "settings": {
                "foreground": "#97281c"
            }
        },
        {
            "name": "Keywords: context-sensitive",
            "scope": [
                "keyword.other.context"
            ],
            "settings": {
                "foreground": "#db0054"
            }
        },
        // {
        //     "name": "Keywords: types",
        //     "scope": [
        //         "storage.type.type"
        //     ],
        //     "settings": {
        //         "foreground": "#0a8a8f"
        //     }
        // },
        // {
        //     "name": "Keywords: cv-qualifiers",
        //     "scope": [
        //         "storage.modifier.cv"
        //     ],
        //     "settings": {
        //         "foreground": "#0050c7"
        //     }
        // },
        {
            "name": "Attributes",
            "scope": [
                "meta.attribute.region"
            ],
            "settings": {
                "foreground": "#640046"
            }
        },
        {
            "name": "Attributes (names)",
            "scope": [
                "storage.modifier.attrib"
            ],
            "settings": {
                "foreground": "#ff25be"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {
                "foreground": "#b40797"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric",
                "constant"
            ],
            "settings": {
                "foreground": "#058516"
            }
        },
        // {
        //     "name": "Numbers (alt)",
        //     "scope": [
        //         "constant.numeric.real.dec.exp",
        //         "constant.numeric.real.hex.exp",
        //         "constant.numeric.real.dec.prefix",
        //         "constant.numeric.real.hex.prefix",
        //         "constant.numeric.int.hex.prefix",
        //         "constant.numeric.int.oct.prefix",
        //         "constant.numeric.int.bin.prefix"
        //     ],
        //     "settings": {
        //         "foreground": "#04410c"
        //     }
        // },
        {
            "name": "Characters",
            "scope": [
                "string.quoted.single",
            ],
            "settings": {
                "foreground": "#008f99"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#c41f1f"
            }
        },
        {
            "name": "Strings: raw",
            "scope": "string.quoted.triple",
            "settings": {
                "foreground": "#464646"
            }
        },
        {
            "name": "Strings: raw (delimiter)",
            "scope": "string.quoted.triple.delim",
            "settings": {
                "foreground": "#d16b17"
            }
        },
        {
            "name": "Strings: escape sequences",
            "scope": "constant.character.escape",
            "settings": {
                "foreground": "#002981"
            }
        },
        {
            "name": "Invalid & illegal (escape sequences)",
            "scope": "invalid.illegal.escape",
            "settings": {
                "foreground": "#ff00ff"
            }
        },
        {
            "name": "Literal suffixes",
            "scope": [
                "constant.other.suffix"
            ],
            "settings": {
                // "foreground": "#c7580f"
                "foreground": "#d35005"
                // "foreground": "#c24700"
                // "foreground": "#05423a"
                // "foreground": "#1c8aac"
                // "foreground": "#058516"
            }
        },
    ]
}
